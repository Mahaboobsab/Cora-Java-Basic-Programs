		String s2= "20";
		String s3= s1+s2;
		System.out.println("value of s1 is"+s1);
		System.out.println("value of s2 is"+s2);
  	    System.out.println("value of s3 is"+s3);
		int x=Integer.parseInt(s1);
        int y=Integer.parseInt(s2);
	    int z = x + y;
		System.out.println("value of x is"+x);
