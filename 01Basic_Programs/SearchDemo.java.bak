class SearchDemo 
{
	public static void main(String[] args) 
	{
		System.out.println("Hello World!");
		int num []={6,8,3,7,5,6,1,4 };
		int value = 5;
		boolean found = false;
	    for (int x:num)
	    {
			if (x == value)
			{
				found = true;
					break;
				}
	          }
        if (found)
        {
        System.out.println("value found");
        }
		else
        {
         System.out.println("value not found");
        }
	}
}
